@model List<ProgramWeb.Models.ViewModel.UserInfoViewModel>
<!-- Temporary page for testing of tables -->
@{
    ViewBag.Title = "Table testing (temp)";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<table class="table table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>UserName</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Email</td>
                <td>@item.UserName</td>
            </tr>
        }
    </tbody>
</table>